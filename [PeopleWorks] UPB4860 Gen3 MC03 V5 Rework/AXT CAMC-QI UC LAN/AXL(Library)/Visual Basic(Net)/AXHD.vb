    Function QI_SND_EVENT_AXIS(ByVal Axis As Long) As Long              ' bit 19~18 (00:X, 01:Y, 10:Z, 11:U)
        QI_SND_EVENT_AXIS = ((Axis Mod 4) * (2 ^ 18))
    End Function

    Function QI_FST_EVENT_AXIS(ByVal Axis As Long) As Long              ' bit 17~16 (00:X, 01:Y, 10:Z, 11:U)
        QI_FST_EVENT_AXIS = ((Axis Mod 4) * (2 ^ 16))
    End Function

    Function QI_OPERATION_EVENT_2(ByVal Event1 As Long) As Long         ' bit 15..8
        QI_OPERATION_EVENT_2 = ((Event1 And &HFF) * (2 ^ 8))
    End Function

    Function QI_OPERATION_EVENT_1(ByVal Event1 As Long) As Long         ' bit 7..0
        QI_OPERATION_EVENT_1 = (Event1 And &HFF)
    End Function

    Function QI_OPERATION_COMMAND(ByVal Command As Long, ByVal Axis As Long) As Long
        QI_OPERATION_COMMAND = ((Command And &HFF) * (2 ^ ((Axis Mod 4) * 8)))
    End Function
    
